{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "max-len": ["error", 80],
    "no-duplicate-imports": "off",
    "no-restricted-syntax": "off",
    "generator-star-spacing": "off",
    "import/no-extraneous-dependencies": ["error", {"peerDependencies": true}]
  }
}
